{
  "name": "queryjs",
  "version": "0.3.1",
  "description": "QueryJS - Minimal jQuery like library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": "dist",
  "dist": "dist",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "docs": "rimraf docs && typedoc --out docs src",
    "serve": "node serve",
    "test": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.conf.js",
    "test:chrome": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:chromeHeaded": "karma start --browsers Chrome karma.conf.js",
    "test:firefox": "karma start --single-run --browsers FirefoxHeadless karma.conf.js",
    "test:edge": "karma start --single-run --browsers Edge karma.conf.js",
    "test:ie": "karma start --browsers IE karma.ie.conf.js",
    "test:safari": "karma start --single-run --browsers Safari karma.conf.js",
    "test:watch": "karma watch --browsers Chrome karma.conf.js"
  },
  "author": "AaronNGray",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/karma-fixture": "^0.2.5",
    "@types/mocha": "^2.2.41",
    "chai": "^4.2.0",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^2.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-typescript": "^5.0.3",
    "karma-typescript-es6-transform": "^4.1.1",
    "karma-typescript-preprocessor": "^0.3.1",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.25.2",
    "serve-index": "^1.9.1",
    "serve-static": "^1.14.1",
    "sinon": "^8.0.4",
    "standard": "^10.0.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.6"
  }
}
